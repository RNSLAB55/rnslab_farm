{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react","prettier"
    ],
    "rules": {
        "arrow-body-style": ["error", "always"],
        "prettier/prettier": ["error", { "endOfLine": "auto" }] 
    }
}
